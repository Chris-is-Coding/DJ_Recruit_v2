<p>
  <strong>Title:</strong>
  <%= @job_posting.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @job_posting.content %>
</p>

<p>
  <strong>Start Time:</strong>
  <%= @job_posting.start_time %>
  <br>
  <strong>End Time:</strong>
  <%= @job_posting.end_time %>
</p>

<p>
  <strong>Hourly Rate:</strong>
  <%= @job_posting.hourly_rate %>
</p>

<p>
  <strong>Skills:</strong>
  <% @job_posting.skills.each do |skill| %>
  <li>
    <%= skill.name %>
  </li>
<% end %>
</p>
