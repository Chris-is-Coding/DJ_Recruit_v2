<div class='jumbotron text-center'>
<h1>Profile</h1>
<br>
  <p>First name: <%= @employee.first_name %></p>
  <p>Last name: <%= @employee.last_name %></p>
  <p>E-mail: <%= @employee.email %></p>
  <p>Phone number: <%= @employee.phone_number %></p>

 <% if @employee %>
    <p><%= link_to 'Edit Profile',edit_employee_registration_path, class: 'btn btn-primary' %></p>
  <% else %>
    <p><%= link_to 'Login', new_employee_session_path, class: 'btn btn-primary' %></p>
<% end %>
</div>