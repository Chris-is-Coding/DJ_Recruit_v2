<h1>My profile</h1>

  <p>First name: <%= @employee.first_name %></p>
  <p>Last name: <%= @employee.last_name %></p>
  <p>E-mail: <%= @employee.email %></p>
  <p>Phone number: <%= @employee.phone_number %></p>

  <%= image_tag(@employee.image) %>

 <% if @employee %>
    <li><%= link_to 'Edit Profile',edit_employee_registration_path %></li>
  <% else %>
    <li><%= link_to 'Login', new_employee_session_path %></li>
<% end %>